{
  "compilerOptions": {
    "target": "es5",
    "types": [
      "node"
    ],
    "typeRoots": [
      "./node_modules/@types/"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "node_modules"
  ]
}